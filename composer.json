{
    "name": "project-ufa/yii2-cocos2d-html5",
    "description": "Cocos2d-html estension for yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","cocos2d","html5"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/project-ufa/yii2-cocos2d-html5/issues",
         "source": "https://github.com/project-ufa/yii2-cocos2d-html5"
    },
    "authors": [
        {
            "name": "Juan C Robles",
            "email": "project.ufa@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "bower-asset/cocos2d-html5": "3.*"
    },
    "autoload": {
        "psr-4": {
            "projectufa\\cocos2dhtml5\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
